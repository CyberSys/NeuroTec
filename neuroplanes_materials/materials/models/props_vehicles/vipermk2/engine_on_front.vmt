		}

		"Sine"
		{
			"resultVar" "$color"
			"sineperiod" 1.2
			"sinemin" 2
			"sinemax" 4
		}

		"LinearRamp"
		{
			"rate" -1045
			"initialValue" 0.0
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
//			"rotateVar" "$angle"
			"centerVar" "$center"
			"scaleVar"	"$tex2scale"
			"resultVar"	"$basetexturetransform"
		}

		"TextureTransform"
		{
			"rotateVar" "$angle"
			"centerVar" "$center"
//			"scaleVar"	"$tex2scale"
			"resultVar"	"$texture2transform"
		}
	}
